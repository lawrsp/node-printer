  },
  "licenses": [
    {
      "type": "BSD"
    }
  ],
  "main": "./lib/printer",
  "dependencies": {
    "nan": "^2.18.0",
    "prebuild-install": "^7.1.1"
  },
  "files": ["lib", "index.d.ts", "printer.js", "binding.gyp"]
